<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net5.0;;net6.0</TargetFrameworks>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<Configurations>Debug;Release;Debug_Unimake</Configurations>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageId>Unimake.MessageBroker</PackageId>
		<Title>Unimake.MessageBroker</Title>
		<Authors>Unimake</Authors>
		<Company>Unimake</Company>
		<Product>Unimake.MessageBroker</Product>
		<Description>Pacote de envio de mensagens, SNS e notificações via WhatsApp, Messenger, e-mail. Entre outros serviços de mensagens.</Description>
		<Copyright>Unimake 2023</Copyright>
		<PackageProjectUrl>https://unimake.app/mb</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Unimake/Unimake.MessageBroker</RepositoryUrl>
		<PackageTags>Unimake;MessageBroker;Message;SNS;Broker;WhatsApp;Messenger;Direct</PackageTags>
		<AssemblyVersion>2023.05.03.1527</AssemblyVersion>
		<Version>20230503.1527.16</Version>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Unimake.AuthServer" Version="20230503.1527.16" />
		<PackageReference Include="Unimake.MessageBroker.Primitives" Version="20230503.1527.16" />
	</ItemGroup>

</Project>
